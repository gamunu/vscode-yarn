{
  "name": "vscode-yarn",
  "displayName": "yarn",
  "description": "Yarn commands for VSCode",
  "version": "2.2.0",
  "publisher": "gamunu",
  "icon": "yarn_icon.png",
  "engines": {
    "vscode": "^1.75.0",
    "node": ">=18.0.0"
  },
  "categories": [
    "Other"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/gamunu/vscode-yarn/issues"
  },
  "homepage": "https://github.com/gamunu/vscode-yarn/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/gamunu/vscode-yarn.git"
  },
  "keywords": [
    "multi-root ready"
  ],
  "main": "./dist/extension",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "yarn-sidebar",
          "title": "Yarn",
          "icon": "./assets/yarn-icon.svg"
        }
      ]
    },
    "views": {
      "yarn-sidebar": [
        {
          "id": "yarnSidebar",
          "name": "Search Packages",
          "icon": "./assets/install.png"
        },
        {
          "id": "package-dependencies",
          "name": "Dependencies",
          "icon": "./assets/install.png"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "yarnSidebar",
        "contents": "No search results yet. Use the search box above to find npm packages.\n[Learn More](https://github.com/gamunu/vscode-yarn/blob/master/README.md)"
      }
    ],
    "commands": [
      {
        "command": "yarn-script.start",
        "title": "Start",
        "category": "Yarn",
        "icon": "./assets/start.png"
      },
      {
        "command": "yarn-script.test",
        "title": "Test",
        "category": "Yarn",
        "icon": "./assets/test.png"
      },
      {
        "command": "yarn-script.terminate",
        "title": "Terminate Running Script",
        "category": "Yarn"
      },
      {
        "command": "yarn-script.installPackages",
        "title": "Install Packages",
        "category": "Yarn",
        "icon": "./assets/install.png"
      },
      {
        "command": "yarn-script.addPackage",
        "title": "Add & Save Dependency",
        "category": "Yarn"
      },
      {
        "command": "yarn-script.addPackageDev",
        "title": "Add & Save dev. Dependency",
        "category": "Yarn"
      },
      {
        "command": "yarn-script.runScript",
        "title": "Run Script",
        "category": "Yarn"
      },
      {
        "command": "yarn-script.runScriptLast",
        "title": "Run Last Script",
        "category": "Yarn"
      },
      {
        "command": "yarn-script.init",
        "title": "Initialize Package",
        "category": "Yarn"
      },
      {
        "command": "yarn-script.outdated",
        "title": "Outdated",
        "category": "Yarn"
      },
      {
        "command": "yarn-script.removePackage",
        "title": "Uninstall Dependency",
        "category": "Yarn"
      },
      {
        "command": "yarn-script.build",
        "title": "Build",
        "category": "Yarn",
        "icon": "./assets/build.png"
      },
      {
        "command": "yarn-script.publish",
        "title": "Publish Package",
        "category": "Yarn"
      },
      {
        "command": "yarn-script.raw",
        "title": "Raw Command",
        "category": "Yarn"
      },
      {
        "command": "yarn-script.searchPackages",
        "title": "Search Packages",
        "category": "Yarn"
      },
      {
        "command": "yarn-script.refreshSidebar",
        "title": "Refresh Yarn Sidebar",
        "category": "Yarn",
        "icon": "$(refresh)"
      },
      {
        "command": "yarn-script.searchInSidebar",
        "title": "Search Packages in Sidebar",
        "category": "Yarn",
        "icon": "$(search)"
      },
      {
        "command": "yarn-script.showInstalledPackages",
        "title": "Show Installed Packages",
        "category": "Yarn",
        "icon": "$(package)"
      },
      {
        "command": "yarn-script.installPackageFromSidebar",
        "title": "Install Package",
        "category": "Yarn",
        "icon": "$(cloud-download)"
      },
      {
        "command": "yarn-script.installDevPackageFromSidebar",
        "title": "Install as Dev Dependency",
        "category": "Yarn",
        "icon": "$(tools)"
      },
      {
        "command": "yarn-script.uninstallPackageFromSidebar",
        "title": "Uninstall Package",
        "category": "Yarn",
        "icon": "$(trash)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceFilename == 'package.json'",
          "group": "navigation@1",
          "submenu": "vscode.yarn"
        }
      ],
      "vscode.yarn": [
        {
          "command": "yarn-script.installPackages",
          "group": "navigation@1",
          "submenu": "vscode.yarn"
        },
        {
          "command": "yarn-script.test",
          "group": "navigation@2",
          "submenu": "vscode.yarn"
        },
        {
          "command": "yarn-script.build",
          "group": "navigation@3",
          "submenu": "vscode.yarn"
        },
        {
          "command": "yarn-script.start",
          "group": "navigation@4",
          "submenu": "vscode.yarn"
        },
        {
          "command": "yarn-script.runScript",
          "group": "navigation@5",
          "submenu": "vscode.yarn"
        },
        {
          "command": "yarn-script.addPackage",
          "group": "navigation@6",
          "submenu": "vscode.yarn"
        },
        {
          "command": "yarn-script.addPackageDev",
          "group": "navigation@7",
          "submenu": "vscode.yarn"
        },
        {
          "command": "yarn-script.removePackage",
          "group": "navigation@8",
          "submenu": "vscode.yarn"
        },
        {
          "command": "yarn-script.publish",
          "group": "navigation@9",
          "submenu": "vscode.yarn"
        },
        {
          "command": "yarn-script.outdated",
          "group": "navigation@10",
          "submenu": "vscode.yarn"
        },
        {
          "command": "yarn-script.searchPackages",
          "group": "navigation@11",
          "submenu": "vscode.yarn"
        }
      ],
      "view/title": [
        {
          "command": "yarn-script.refreshSidebar",
          "when": "view == yarnSidebar",
          "group": "navigation"
        },
        {
          "command": "yarn-script.searchInSidebar",
          "when": "view == yarnSidebar",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "yarn-script.searchInSidebar",
          "when": "viewItem == search-box",
          "group": "inline"
        },
        {
          "command": "yarn-script.installPackageFromSidebar",
          "when": "viewItem == package",
          "group": "inline@1"
        },
        {
          "command": "yarn-script.installDevPackageFromSidebar",
          "when": "viewItem == package",
          "group": "inline@2"
        },
        {
          "command": "yarn-script.uninstallPackageFromSidebar",
          "when": "viewItem == installed-package || viewItem == installed-dev-package",
          "group": "inline"
        }
      ],
      "touchBar": [
        {
          "command": "yarn-script.installPackages",
          "group": "navigation@+1",
          "when": "config.yarn.enableTouchbar"
        },
        {
          "command": "yarn-script.build",
          "group": "navigation@+4",
          "when": "config.yarn.enableTouchbar"
        },
        {
          "command": "yarn-script.test",
          "group": "navigation@+3",
          "when": "config.yarn.enableTouchbar"
        },
        {
          "command": "yarn-script.start",
          "group": "navigation@+2",
          "when": "config.yarn.enableTouchbar"
        },
        {
          "command": "yarn-script.searchInSidebar",
          "group": "navigation@+5",
          "when": "config.yarn.enableTouchbar"
        }
      ]
    },
    "submenus": [
      {
        "id": "vscode.yarn",
        "label": "Yarn"
      }
    ],
    "keybindings": [
      {
        "command": "yarn-script.runScript",
        "key": "Ctrl+R Shift+R",
        "mac": "Cmd+R Shift+R",
        "when": "!terminalFocus"
      },
      {
        "command": "yarn-script.runScriptLast",
        "key": "Ctrl+R R",
        "mac": "Cmd+R R",
        "when": "!terminalFocus"
      },
      {
        "command": "yarn-script.terminate",
        "key": "Ctrl+R Shift+X",
        "mac": "Cmd+R Shift+X",
        "when": "!terminalFocus"
      },
      {
        "command": "yarn-script.test",
        "key": "Ctrl+R T",
        "mac": "Cmd+R T",
        "when": "!terminalFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "yarn (VSCode Yarn)",
      "properties": {
        "yarn.runInTerminal": {
          "type": "boolean",
          "scope": "resource",
          "default": true,
          "description": "Run yarn commands in a terminal, otherwise shows the output in the output panel"
        },
        "yarn.bin": {
          "type": "string",
          "default": "yarn",
          "description": "yarn bin name"
        },
        "yarn.packageJson": {
          "type": "string",
          "scope": "resource",
          "default": "package.json",
          "description": "Default package json path. relative to current project root"
        },
        "yarn.dontHideOutputOnSuccess": {
          "type": "boolean",
          "scope": "resource",
          "default": true,
          "description": "Keep the output panel visible when yarn execution is successful (no effect with runInTerminal)"
        },
        "yarn.enableTouchbar": {
          "type": "boolean",
          "scope": "resource",
          "default": false,
          "description": "Enable yarn commands in the macOS touchbar."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "eslint": "eslint -c .eslintrc.js --ext .ts src/*.ts"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.58.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "global": "^4.3.2",
    "run-in-terminal": "^0.0.3",
    "tree-kill": "^1.2.2",
    "node-fetch": "^2.6.7"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}